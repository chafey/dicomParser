{
  "name": "dicom-parser",
  "version": "1.8.5",
  "description": "Javascript parser for DICOM Part 10 data",
  "main": "dist/dicomParser.js",
  "module": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "DICOM",
    "medical",
    "imaging"
  ],
  "author": "Chris Hafey",
  "homepage": "https://github.com/cornerstonejs/dicomParser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cornerstonejs/dicomParser/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cornerstonejs/dicomParser.git"
  },
  "scripts": {
    "build": "npm run test && npm run version && npm run build-library && npm run copy:meteor && npm run doc:generate",
    "clean": "npm run clean:dist && npm run clean:coverage",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "clean:docs": "shx rm -rf documentation",
    "copy:meteor": "shx cp ./meteor/dicomParser/package.js ./dist/",
    "doc": "npm run doc:generate && opn documentation/index.html",
    "doc:generate": "npm run clean:docs && typedoc",
    "version": "node -p -e \"'export default \\'' + require('./package.json').version + '\\';'\" > src/version.ts",
    "test": "npm run test-local && npm run test-browser:chrome",
    "test-local": "mocha --require mocha.test.bootsrap.js test/*_test.ts",
    "test-browser:chrome": "karma start config/karma/karma-chrome.js",
    "test-browser:firefox": "karma start config/karma/karma-firefox.js",
    "test-browser:watch": "karma start config/karma/karma-watch.js",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "tslint-fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "tslint-quiet": "tslint -c tslint.json --quiet 'src/**/*.ts'",
    "build-webpack": "webpack --config webpack.prod.js",
    "build-lib": "tsc -declarationDir \"dist/types\"",
    "build-library": "npm run build-webpack && npm run build-lib",
    "prebuild": "npm run clean"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "@types/pako": "^1.0.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^5.0.5",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.2",
    "opn-cli": "^5.0.0",
    "puppeteer": "^3.0.4",
    "rimraf": "^3.0.2",
    "rusha": "^0.8.13",
    "shx": "^0.3.2",
    "terser-webpack-plugin": "^3.0.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.17.6",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "pako": "^1.0.11"
  }
}
